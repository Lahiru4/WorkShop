<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.net.URL?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="195.0" prefWidth="510.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.cashier.PlayBillController">
   <children>
      <JFXButton fx:id="printBill" contentDisplay="TOP" layoutX="73.0" layoutY="40.0" prefHeight="95.0" prefWidth="116.0" text="Print Bill">
         <graphic>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../img/icons8-print-48.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="viewBill" contentDisplay="TOP" layoutX="297.0" layoutY="40.0" onAction="#viewBillOnAction" prefHeight="95.0" prefWidth="116.0" text="View Bill">
         <graphic>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../img/icons8-preview-pane-48.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
   </children>
   <stylesheets>
      <URL value="@../../style/TexFeld.css" />
      <URL value="@../../style/Cashier.css" />
   </stylesheets>
</AnchorPane>
